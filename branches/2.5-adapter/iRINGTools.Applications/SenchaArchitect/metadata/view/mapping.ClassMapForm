            },
            "codeClass": null,
            "userConfig": {
                "fn": "onSave",
                "implHandler": [
                    "var me = this;\r",
                    "var win = me.up('window');\r",
                    "var form = me.getForm();\r",
                    "var message;\r",
                    "if (form.findField('identifier').getValue() == 'Drop property node(s) here.' ||\r",
                    "form.findField('objectName').getValue() == '' ||\r",
                    "form.findField('className').getValue() == 'Drop a class node here.') {\r",
                    "  message = 'Required fields can not be blank!';\r",
                    "  showDialog(400, 100, 'Warning', message, Ext.Msg.OK, null);\r",
                    "  return;\r",
                    "}\r",
                    "\r",
                    "if(me.getForm().isValid()) {\r",
                    "  me.submit({\r",
                    "    waitMsg: 'Saving Data...',\r",
                    "    success: function (result, request) {\r",
                    "    \twin.fireEvent('save', me);\r",
                    "    },\r",
                    "    failure: function (result, request) {\r",
                    "      message = 'Failed to Add ClassMap to RoleMap';\r",
                    "      showDialog(400, 100, 'Warning', message, Ext.Msg.OK, null);\r",
                    "    }\r",
                    "  });\r",
                    "} else {\r",
                    "  message = 'Form is not complete. Cannot save record.';\r",
                    "  showDialog(400, 100, 'Warning', message, Ext.Msg.OK, null);   \r",
                    "}\r",
                    "\r",
                    ""
                ]
            },
            "designerId": "7b6e8428-b518-4f6b-a90b-9d2b5b69ee01"
        },
