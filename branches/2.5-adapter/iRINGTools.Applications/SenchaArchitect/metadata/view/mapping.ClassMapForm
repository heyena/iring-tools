        {
            "type": "basicfunction",
            "reference": {
                "name": "items",
                "type": "array"
            },
            "codeClass": null,
            "userConfig": {
                "fn": "updateDDContainers",
                "designer|params": [
                    "record"
                ],
                "implHandler": [
                    "var me = this;\r",
                    "var pcon = me.down('#cmpcontainer');\r",
                    "var ccon = me.down('#cmccontainer');\r",
                    "var identifier = 'Drop property node(s) here.';\r",
                    "var classlabel = 'Drop a class node here.';\r",
                    "var delimeter = '_';\r",
                    "if (record != null && record.classMap != null) {\r",
                    "  identifier =  record.classMap.identifiers[0];\r",
                    "  if(record.classMap.identifiers.length>1){\r",
                    "    for(var i=1;i<record.classMap.identifiers.length;i++){\r",
                    "      identifier = identifier+','+record.classMap.identifiers[i];\r",
                    "    }\r",
                    "  }\r",
                    "  //identifier = getLastXString(record.record.classTemplateMaps[0].classMap.identifiers[0], 1).split('.')[1];\r",
                    "  //identifier = record.classTemplateMaps[0].classMap.identifiers[0];\r",
                    "  classlabel = record.classMap.name;\r",
                    "  if(record.classMap.identifierDelimiter!=null)\r",
                    "  delimeter = record.classMap.identifierDelimiter;\r",
                    "}\r",
                    "//if(record!=null)\r",
                    "//identifier = 'Identifier: ' + identifier;\r",
                    "\r",
                    "pcon.update(identifier);\r",
                    "ccon.update(classlabel);\r",
                    "me.getForm().findField('identifier').setValue(identifier);\r",
                    "me.getForm().findField('className').setValue(classlabel);\r",
                    "me.getForm().findField('delimeter').setValue(delimeter);\r",
                    "//if(record!=null)\r",
                    "//var classlabel = 'Class Label: ' + label;\r",
                    "// else\r",
                    "// var classlabel = 'Drop a class node here.';\r",
                    "\r",
                    "\r",
                    "//ccon.update(classlabel);"
                ]
            },
            "designerId": "60ce47f1-7481-41ab-863b-3ce956e48c44"
        }
    ]
