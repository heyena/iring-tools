                    "    var error = 'SUCCESS = FALSE';\r",
                    "    var index = rtext.toUpperCase().indexOf(error);\r",
                    "    var msg = rtext.substring(index + error.length + 2, rtext.length - 1);\r",
                    "    showDialog(500, 300, 'Error', msg, Ext.Msg.OK, null);\r",
