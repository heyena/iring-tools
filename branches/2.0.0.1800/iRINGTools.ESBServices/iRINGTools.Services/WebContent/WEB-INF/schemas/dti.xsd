<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://iringtools.org/adapter/dti"
  targetNamespace="http://iringtools.org/adapter/dti" elementFormDefault="qualified" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
  jaxb:version="2.0" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" jaxb:extensionBindingPrefixes="xjc">

  <xs:annotation>
    <xs:appinfo>
      <jaxb:globalBindings>
        <xjc:simple/>
      </jaxb:globalBindings>
    </xs:appinfo>
  </xs:annotation>

  <xs:element name="dataTransferIndices" type="DataTransferIndices"/>

  <xs:complexType name="DataTransferIndices">
    <xs:sequence>
      <xs:element name="dataTransferIndex" type="DataTransferIndex" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="DataTransferIndex">
    <xs:sequence>
      <xs:element name="identifier" type="xs:string"/>
      <xs:element name="hashValue" type="xs:string"/>
      <xs:element name="transferType" type="TransferType"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="TransferType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Sync"/>
      <xs:enumeration value="Add"/>
      <xs:enumeration value="Change"/>
      <xs:enumeration value="Delete"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>