<?xml version="1.0" encoding="utf-8"?>
<configration xmlns="http://schemas.datacontract.org/2004/07/org.iringtools.oracle" xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
  <provider>ORACLE</provider>
  <connection>Data Source=MMSTST;User Id=hatch_sys;Password=manager;</connection>
  <objects>
    <object>
      <name>Valves</name>
      <identifier>IDENT</identifier>      
      <select>        
        <query>select * from vw_iring_valves where (rownum &lt; 10)</query>        
      </select>
      <update>        
        <query>update vw_iring_valves set tag_number = @tag_number where ident = @identifier</query>
        <parameters>
          <parameter>
            <name>@identifier</name>
            <type>Int32</type>
          </parameter>
          <parameter>
            <name>@tag_number</name>
            <type>String</type>
          </parameter>
        </parameters>
      </update>
      <delete>        
        <query>delete from vw_iring_valves where ident = @identifier</query>
        <parameters>
          <parameter>
            <name>@identifier</name>
            <type>Int32</type>
          </parameter>
        </parameters>
      </delete>
      <insert>        
        <query>insert into vw_iring_valves (ident, tag_number) values (@identifier, @tag_number)</query>
        <parameters>
          <parameter>
            <name>@identifier</name>
            <type>Int32</type>
          </parameter>
          <parameter>
            <name>@tag_number</name>
            <type>String</type>
          </parameter>
        </parameters>
      </insert>
    </object>
  </objects>
</configration>