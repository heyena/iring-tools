    <add key="SSO_URL" value="https://sso.staging.mypsn.com/as/token.oauth2" />
    <add key="client_id" value="IW_DataLayer" />
    <add key="client_secret" value="tmPhR7ZNngdXtsQrmEKhLhi7wa7mHhG0GX3Zn9HvillzCCwf5b/JDMC/a65QkeB6" />
