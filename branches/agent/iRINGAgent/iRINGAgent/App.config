    <add key="AgentConfig" value="D:\Dev\svn\branches\agent\iRINGAgent\iRINGAgent\agentconfig.xml"/> <!--"..\iRINGAgent\iRINGAgent\agentconfig.xml"/> -->
    <add key="RequestTimeout" value="3600000"/>
  </appSettings>

  <log4net>
    <appender name="LogFile" type="log4net.Appender.RollingFileAppender">
      <file value=".\iRINGAgent.log" />
      <appendToFile value="true" />
      <maximumFileSize value="100KB" />
      <maxSizeRollBackups value="10" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>

    <!-- Log Level:
		        0 FATAL
		        1 ERROR
		        2 WARN
		        3 INFO
		        4 DEBUG
		        Messages from lower levels are included.  DEBUG will log everything!
	      -->

    <logger name="org.iringtools">
      <level value="DEBUG" />
      <appender-ref ref="LogFile" />
    </logger>
    <logger name="iRINGAgent">
      <level value="DEBUG" />
      <appender-ref ref="LogFile" />
    </logger>
  </log4net>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  
</configuration>