<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<template>
		<objecttype>212</objecttype>
		<name>TMP-MECH-{0}-{1}</name>
		<placeholders>
			<placeholder id="0">CommGrpCode</placeholder>
			<placeholder id="1">CommodityCode</placeholder>
		</placeholders>
	</template>
	<mappings>
		<map property="Code" type="3"/>
		<map property="Unit" type="2">
			<rule order="0">5</rule>
			<rule order="1">20</rule>
		</map >
		<map property="SafetyDesignator" type="1"/>
		<map property="System" type="2">
			<rule order="0">1</rule>
		</map>
    <!--<map property="CommGrpCode" type="0"/>-->
    <map property="CommGrpCode" type="1"/>
		<map property="Description" type="5"/>
		<map property="ComponentFunctionName(CFN)" type="1"/>
		<map property="P&amp;ID" type="2">
			<rule order="0">2</rule>
		</map>
		<!--<map property="P&amp;IDRev" type="0"/>-->
		<map property="EquipmentRating" type="1"/>
		<map property="ReferenceCalc" type="2"/>
		<map property="CommodityCode" type="1"/>
		<map property="MaterialReqNo" type="2"/>
		<map property="DataSheetNo" type="2"/>
		<map property="PowerReqd" type="1"/>
		<map property="HP(Est/Act/Calc)" type="1"/>
		<map property="ApprovedLoad" type="1"/>
    <!--<map property="LOADUOM" type="0"/>-->
		<map property="Manufacturer" type="1"/>
		<map property="ModelNo" type="1"/>
		<map property="VendorDrawing" type="2"/>
		<map property="Area/VNS" type="2">
			<rule order="0">3</rule>
			<rule order="1">25</rule>
		</map >
		<map property="PONumber" type="2"/>
		<map property="VendorSupplied(Y/N)" type="1"/>
		<map property="Q-List(Y/N)" type="1"/>
		<map property="ApprovedSeismicClass" type="1"/>
		<map property="ApprovedPressureBoundryQualityGroup" type="1"/>
		<map property="ApprovedNon-PressureBoundryQualityGroup" type="1"/>
		<map property="ApprovedQualityClass" type="1"/>
		<map property="ApprovedAQClass" type="1"/>
		<!--<map property="Facility/BLDG" type="0"/>-->
		<!--<map property="Location Elev/Area" type="0"/>-->
	</mappings>
</configuration>
