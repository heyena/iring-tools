<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<template>
		<objecttype>212</objecttype>
		<name>TMP-LINE-{0}</name>
		<placeholders>
			<placeholder id="1">Unit</placeholder>
		</placeholders>
	</template>
	<mapppings>
		<map columnname="Tag Number" destination="3"/>
		<map columnname="Unit" destination="2"/>
		<map columnname="Safety Designator" destination="1"/>
		<map columnname="System" destination="2">
			<rule order="0">1</rule>
			<rule order="1">18</rule>
		</map >
		<map columnname="Comm Grp Code" destination="1"/>
		<map columnname="Line Class" destination="0"/>
		<map columnname="Diameter" destination="1"/>
		<map columnname="Dia. UOM" destination="0"/>
		<map columnname="Vendor (Y/N)" destination="1"/>
		<map columnname="Heat Trace" destination="1"/>
		<map columnname="P&amp;ID" destination="2">
			<rule order="0">2</rule>
		</map>
		<map columnname="P&amp;ID Rev" destination="0"/>
		<map columnname="Appr. DPress" destination="1"/>
		<map columnname="Pressure UOM" destination="0"/>
		<map columnname="Appr. DTemp" destination="1"/>
		<map columnname="Temp UOM" destination="0"/>
		<map columnname="Insulated (Y/N)" destination="1"/>
		<map columnname="Appr. Insul Material" destination="1"/>
		<map columnname="Appr. Insulation Thickness" destination="1"/>
		<map columnname="Insul UOM" destination="0"/>
		<map columnname="Q-List (Y/N)" destination="1"/>
		<map columnname="Approved Seismic Class" destination="1"/>
		<map columnname="Approved Pressure Boundry Quality Group" destination="1"/>
		<map columnname="Approved Quality Class" destination="1"/>
		<map columnname="Approved AQ Class" destination="1"/>
	</mapppings>
</configuration>
