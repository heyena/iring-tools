                    "var me = this;\r",
                    "if (me.dataTypes === undefined)\r",
                    "return;\r",
                    "\r",
                    "var i = 0;\r",
                    "\r",
                    "while (me.dataTypes[i] === undefined)\r",
                    "i++;\r",
                    "\r",
                    "for (var k = i; k < me.dataTypes.length; k++) {\r",
                    "  if (me.dataTypes[k][1] == dataType)\r",
