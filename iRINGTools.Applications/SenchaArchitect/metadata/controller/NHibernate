                    "  params.serName = serName;\r",
                    "  params.contextName = dbObjectsTree.dirNode.data.record.context;\r",
                    "  params.endpoint = dbObjectsTree.dirNode.data.record.endpoint;\r",
                    "  params.baseUrl = dbObjectsTree.dirNode.data.record.BaseUrl;\r",
                    "}, me);\r",
                    "\r",
                    "panel.body.mask('Loading...', 'x-mask-loading');\r",
                    "treeStore.load();\r",
                    "panel.body.unmask();"
