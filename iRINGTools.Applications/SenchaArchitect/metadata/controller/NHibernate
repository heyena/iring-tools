                    "      tagProps.columnName = keyNodeProf.columnName;\r",
                    "      tagProps.propertyName = keyNode.data.text;\r",
                    "\r",
                    "      if (typeof keyNodeProf.dataType == 'string')\r",
                    "      tagProps.dataType = me.getDataTypeIndex(keyNodeProf.dataType);\r",
                    "      else\r",
