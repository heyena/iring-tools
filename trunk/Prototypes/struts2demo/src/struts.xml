<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="directory" extends="json-default">
    <action name="directory" class="org.iringtools.controllers.DirectoryController" method="getDirectory">
      <result type="json">
        <param name="root">directoryTree.nodes</param>
      </result>
    </action>
  </package>
  
  <package name="app-data" extends="json-default">
    <action name="adata" class="org.iringtools.controllers.AppDataController" method="getPageDto">
      <result type="json">
         <param name="root">pageDtoGrid</param>
      </result>
    </action>
  </package>
   
  <package name="exchange-data" extends="json-default">
    <action name="xdata" class="org.iringtools.controllers.ExchangeDataController" method="getPageDto">
      <result type="json">
         <param name="root">pageDtoGrid</param>
      </result>
    </action>
	</package>
</struts>