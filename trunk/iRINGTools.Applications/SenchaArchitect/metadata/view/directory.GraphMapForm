            "userConfig": {
                "fn": "onSave",
                "implHandler": [
                    "var me = this;\r",
                    "var win = me.up('window');\r",
                    "if (me.getForm().findField('objectName').getValue() === '' ||\r",
                    "me.getForm().findField('graphName').getValue() === '' ||\r",
                    "me.getForm().findField('className').getValue() === '') {\r",
                    "  showDialog(400, 50, 'Warning', 'Please fill in every field in this form.', Ext.Msg.OK, null);\r",
                    "  return;\r",
                    "}\r",
                    "me.getForm().submit({\r",
                    "  waitMsg: 'Saving Data...',\r",
                    "  success: function (f, a) {\r",
                    "    \r",
                    "    win.fireEvent('save', me);\r",
                    "  },\r",
                    "  failure: function (f, a) {\r",
                    "    var message = 'Error saving changes!';\r",
                    "    showDialog(400, 50, 'Warning', message, Ext.Msg.OK, null);\r",
                    "  }\r",
                    "});"
                ]
            },
            "designerId": "43497a64-024e-48e7-991e-517cfd58754a"
