        {
            "type": "basicfunction",
            "reference": {
                "name": "items",
                "type": "array"
            },
            "codeClass": null,
            "userConfig": {
                "fn": "updateDDContainers",
                "designer|params": [
                    "record"
                ],
                "implHandler": [
                    "var me = this;\r",
                    "var pcon = me.down('#gmfpcontainer');\r",
                    "var ccon = me.down('#gmfccontainer');\r",
                    "var identifier = 'Drop property node(s) here.';\r",
                    "var classlabel = 'Drop a class node here.';\r",
                    "if (record != null) {\r",
                    "  identifier =  record.classTemplateMaps[0].classMap.identifiers[0];\r",
                    "  if(record.classTemplateMaps[0].classMap.identifiers.length>1){\r",
                    "    for(var i=1;i<record.classTemplateMaps[0].classMap.identifiers.length;i++){\r",
                    "      identifier = identifier+','+record.classTemplateMaps[0].classMap.identifiers[i];\r",
                    "    }\r",
                    "  }\r",
                    "  //identifier = getLastXString(record.record.classTemplateMaps[0].classMap.identifiers[0], 1).split('.')[1];\r",
                    "  //identifier = record.classTemplateMaps[0].classMap.identifiers[0];\r",
                    "  classlabel = record.classTemplateMaps[0].classMap.name;\r",
                    "}\r",
                    "//if(record!=null)\r",
                    "//identifier = 'Identifier: ' + identifier;\r",
                    "\r",
                    "pcon.update(identifier);\r",
                    "\r",
                    "//if(record!=null)\r",
                    "//var classlabel = 'Class Label: ' + label;\r",
                    "// else\r",
                    "// var classlabel = 'Drop a class node here.';\r",
                    "\r",
                    "ccon.update(classlabel);\r",
                    "//ccon.update(classlabel);"
                ]
            },
            "designerId": "3a3ab38d-0954-4ffa-bd64-58155b0bb4c7"
        },
