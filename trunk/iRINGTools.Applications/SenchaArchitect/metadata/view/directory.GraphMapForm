        {
            "type": "basiceventbinding",
            "reference": {
                "name": "listeners",
                "type": "array"
            },
            "codeClass": null,
            "userConfig": {
                "fn": "onFormAfterRender",
                "implHandler": [
                    "var me = this;\r",
                    "var ptarget = me.down('#gmfpcontainer');\r",
                    "var ctarget = me.down('#gmfccontainer');\r",
                    "\r",
                    "var propertyDd = new Ext.dd.DropTarget(ptarget.getEl(), {\r",
                    "  ddGroup: 'propertyGroup',\r",
                    "  notifyEnter: function (propertyDd, e, data) {\r",
                    "    if (data.records[0].data.type != 'DataPropertyNode' && data.records[0].data.type != 'KeyDataPropertyNode')\r",
                    "    return this.dropNotAllowed;\r",
                    "    else\r",
                    "    return this.dropAllowed;\r",
                    "  },\r",
                    "  notifyOver: function (propertyDd, e, data) {\r",
                    "    if (data.records[0].data.type != 'DataPropertyNode' && data.records[0].data.type != 'KeyDataPropertyNode')\r",
                    "    return this.dropNotAllowed;\r",
                    "    else\r",
                    "    return this.dropAllowed;\r",
                    "  },  \r",
                    "  notifyDrop: function (propertyDd, e, data) {\r",
                    "    if (data.records[0].data.type != 'DataPropertyNode' && data.records[0].data.type != 'KeyDataPropertyNode') {\r",
                    "      return false;\r",
                    "    }\r",
                    "    else {\r",
                    "      var ident = getLastXString(data.records[0].data.id, 1);\r",
                    "      var object = getLastXString(data.records[0].data.id, 2);\r",
                    "      //var key1 = getLastXString(data.records[0].data.id, 2)\r",
                    "      //var key2 = getLastXString(data.records[0].data.id, 1);\r",
                    "      var key = object+'.'+ident;//key1+'.'+key2;\r",
                    "      if(me.getForm().findField('identifier').getValue()!='Drop property node(s) here.'){\r",
                    "\r",
                    "        var existingIdentifier =  me.getForm().findField('identifier').getValue();\r",
                    "        if(existingIdentifier!=''){\r",
                    "          var tempObjName = existingIdentifier.split('.')[0];\r",
                    "        \tif (object != tempObjName) {\r",
                    "                var message = 'Properties must root from the same data object as graph!';\r",
                    "                showDialog(400, 100, 'Error', message, Ext.Msg.OK, null);\r",
                    "                return false;\r",
                    "        \t}\r",
                    "          \r",
                    "        \tif(existingIdentifier.indexOf(key)!= -1) {\r",
                    "                var message = 'Duplicate properties are not allowed!';\r",
                    "                showDialog(400, 100, 'Error', message, Ext.Msg.OK, null);\r",
                    "                return false;\r",
                    "              }\r",
                    "          key = existingIdentifier+','+ key;\r",
                    "        }else\r",
                    "        key = key;\r",
                    "        me.getForm().findField('identifier').setValue(key);\r",
                    "      }else{\r",
                    "        me.getForm().findField('identifier').setValue(key);\r",
                    "      }\r",
                    "      me.getForm().findField('objectName').setValue(object);\r",
                    "\r",
                    "      //ptarget.update(msg);\r",
                    "      ptarget.update(key);\r",
                    "      return true;\r",
                    "    }\r",
                    "  }   \r",
                    "});\r",
                    "\r",
                    "var classdd = new Ext.dd.DropTarget(ctarget.getEl(), {\r",
                    "  ddGroup: 'refdataGroup',\r",
                    "  notifyEnter: function (classdd, e, data) {\r",
                    "    if (data.records[0].data.type != 'ClassNode')\r",
                    "    return this.dropNotAllowed;\r",
                    "    else\r",
                    "    return this.dropAllowed;\r",
                    "  },\r",
                    "  notifyOver: function (classdd, e, data) {\r",
                    "    if (data.records[0].data.type != 'ClassNode')\r",
                    "    return this.dropNotAllowed;\r",
                    "    else\r",
                    "    return this.dropAllowed;\r",
                    "  },\r",
                    "  notifyDrop: function (classdd, e, data) {\r",
                    "    if (data.records[0].data.type != 'ClassNode') {\r",
                    "      var message = 'Please slect a RDL Class...';\r",
                    "      showDialog(400, 100, 'Warning', message, Ext.Msg.OK, null);\r",
                    "      return false;\r",
                    "    } else {\r",
                    "      var tempClassLabel = me.getForm().findField('className').getValue();\r",
                    "      var tempClassUrl = me.getForm().findField('classId').getValue();\r",
                    "      if (tempClassLabel !== \"\") {\r",
                    "        me.getForm().findField('oldClassLabel').setValue(tempClassLabel);\r",
                    "        me.getForm().findField('oldClassUrl').setValue(tempClassUrl);\r",
                    "      }\r",
                    "      me.getForm().findField('className').setValue(data.records[0].data.record.Label);\r",
                    "      me.getForm().findField('classId').setValue(data.records[0].data.record.Uri);\r",
                    "      var msg = 'Class Label: ' + data.records[0].data.record.Label;\r",
                    "      //ctarget.update(msg);\r",
                    "      ctarget.update(data.records[0].data.record.Label);\r",
                    "      return true;\r",
                    "    }\r",
                    "  } \r",
                    "}); "
                ],
                "name": "afterrender",
                "scope": "me"
            },
            "designerId": "163b79b0-3fe3-45f4-8a93-93335b9aca97"
