                    "      autoLoad: true,\r",
                    "      data: properties\r",
                    "    });\r",
                    "\r",
                    "    form.getForm().setValues(formRecord);\r",
                    "    if(res.virtualPropertyValues!=undefined){\r",
                    "      for(var i=0;i<res.virtualPropertyValues.length;i++){\r",
                    "        var model = Ext.create('AM.model.VirtualPropertyModel');\r",
                    "        model.data.propertyLength = res.virtualPropertyValues[i].length;\r",
                    "        model.data.propertyName = res.virtualPropertyValues[i].propertyName;\r",
                    "        if(res.virtualPropertyValues[i].type == 0)    \r",
                    "        model.data.propertyType = 'Constant';\r",
                    "        else\r",
                    "        model.data.propertyType = 'Property';\r",
                    "        model.data.valueText = res.virtualPropertyValues[i].valueText;\r",
                    "        store.add(model);\r",
                    "      }\r",
                    "    }\r",
                    "\r",
                    "\r",
                    "    win.on('Save', function () {\r",
                    "      win.destroy();\r",
                    "    }, me);\r",
                    "\r",
                    "    win.on('reset', function () {\r",
                    "      win.destroy();\r",
                    "    }, me);\r",
                    "\r",
                    "    win.show();\r",
                    "    //var directoryTree = Ext.widget('directorytree');\r",
                    "    //win.fireEvent('save', me);\r",
                    "    //directoryTree.onReload();\r",
                    "  },\r",
