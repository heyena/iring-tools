                    "var node = tree.getSelectedNode();\r",
                    "form.getForm().findField('oldPropertyName').setValue(node.data.text);\r",
                    "Ext.Ajax.request({\r",
                    "  url: 'AdapterManager/VirtualProperties',\r",
                    "  timeout: 600000,\r",
                    "  params: {\r",
                    "    scope: node.data.id.split('/')[0],\r",
                    "    app: node.data.id.split('/')[1]\r",
                    "    //tree: Ext.JSON.encode(virtualProperty)\r",
                    "  },\r",
                    "  success: function (response, request) {\r",
                    "    var vProperties = Ext.decode(response.responseText).virtualProperties;\r",
                    "    var res = vProperties[0];\r",
                    "    for(j=0;j<vProperties.length;j++){\r",
                    "      if(vProperties[j].propertyName == node.data.text){\r",
                    "        res = vProperties[j];\r",
                    "        break;\r",
                    "      }\r",
                    "    }\r",
                    "\r",
                    "    var properties = [];\r",
                    "    var formRecord = {\r",
                    "      objectName: res.objectName,\r",
                    "      scope:node.data.id.split('/')[0],\r",
                    "      app:node.data.id.split('/')[1],\r",
                    "      propertyName:res.propertyName,\r",
                    "      delimeter:res.delimiter\r",
                    "    };\r",
                    "    var ii=0;\r",
                    "    node.parentNode.eachChild(function(child) {\r",
                    "      properties.push([ii, child.data.text, child.data.property.Name]);\r",
