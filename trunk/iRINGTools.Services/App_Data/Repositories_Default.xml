<repositories xmlns="http://www.iringtools.org/library" xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
  <repository> <!-- note that the order of the elements are important-->
    <encryptedCredentials></encryptedCredentials> <!-- please use the encrypt credentials utility if you need credentials otherwise omit this element-->
    <description>Development Sandbox</description>
    <isReadOnly>false</isReadOnly> <!-- this must only be set to false for your own updatable repository-->
    <name>My Private Sandbox</name> <!-- this name need to be a unique name in the list of repositories-->
    <repositoryType>Camelot</repositoryType> <!-- Possible values are RDSWIP, Camelot and Part8 ... All new Repositories must be Part8 -->
    <uri>http://localhost/Services/SandboxService/query</uri>
    <updateUri>http://localhost/Services/SandboxService/update</updateUri> <!-- only add this element if your repository is updateable-->
  </repository>
  <repository>
    <description>iRING Sandbox</description>
    <isReadOnly>true</isReadOnly>
    <name>iRING Sandbox</name>
    <repositoryType>Camelot</repositoryType>
    <uri>http://www.iringsandbox.org/repositories/sandbox/query</uri>
  </repository>
  <repository>
    <description>RDS/WIP Part 4 Reference Data Library</description>
    <isReadOnly>true</isReadOnly>
    <name>ReferenceData</name>
    <repositoryType>RDSWIP</repositoryType>
    <uri>http://rdl.rdlfacade.org/data</uri>
  </repository>
  <repository>
    <description>EntityTypes Data Library</description>
    <isReadOnly>true</isReadOnly>
    <name>EntityTypes</name>
    <repositoryType>RDSWIP</repositoryType>
    <uri>http://dm.rdlfacade.org/data</uri>
  </repository>
  <repository>
    <description>RDS/Proto and P7 Initial Set Templates</description>
    <isReadOnly>true</isReadOnly>
    <name>Proto and Initial</name>
    <repositoryType>Camelot</repositoryType>
    <uri>http://www.iringsandbox.org/repositories/tempInitSet/query</uri>
  </repository>
</repositories>