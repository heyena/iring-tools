		<pathelement location="../libraries/apache-httpcomponents-4.0/httpmime-4.0.3.jar" />
	</path>

	<target name="generate">
		<taskdef name="xjc" classname="com.sun.tools.xjc.XJCTask" >
			<classpath refid="jaxb.userclasspath" />
		</taskdef>		
		<xjc destdir="src" extension="true">
			<schema dir="WebContent/WEB-INF/schemas" includes="*.xsd" />
      <arg value="-Xcollection-setter-injector" />
		</xjc>
	</target>

	<target name="clean">
		<delete dir="WebContent/WEB-INF/classes" />
	</target>

	<target depends="clean" name="init">
		<mkdir dir="WebContent/WEB-INF/classes" />
		<copy includeemptydirs="false" todir="WebContent/WEB-INF/classes">
			<fileset dir="src">
				<exclude name="**/*.launch" />
				<exclude name="**/.svn/" />
				<exclude name="**/*.java" />
			</fileset>
		</copy>
	</target>

	<target depends="init" name="build">
		<echo message="${ant.project.name}: ${ant.file}" />
		<javac debug="true" debuglevel="${debuglevel}" destdir="WebContent/WEB-INF/classes" source="${source}" target="${target}">
			<src path="src" />
			<classpath refid="cxf-2.2.9.userclasspath" />
			<classpath refid="jsr311-api.userclasspath" />
			<classpath refid="gson-1.4.userclasspath" />
			<classpath refid="log4j-1.2.16.userclasspath" />
			<classpath refid="guice-2.0.userclasspath" />
      <classpath refid="httpcomponents-4.0.userclasspath" />
		</javac>
		<copy todir="WebContent/WEB-INF/lib" flatten="true">
			<path refid="cxf-2.2.9.userclasspath" />
			<path refid="jsr311-api.userclasspath" />
			<path refid="gson-1.4.userclasspath" />
			<path refid="log4j-1.2.16.userclasspath" />
			<path refid="guice-2.0.userclasspath" />
			<path refid="httpcomponents-4.0.userclasspath" />
		</copy>
		<jar destfile="${ant.project.name}.war" basedir="WebContent" excludes="**/.svn/,WEB-INF/logs/*.*" />
	</target>
</project>
