	  <uri>http://localhost:2020/sandbox</uri>
      <updateUri>updateUri</updateUri>
      <namespaces>
      	<namespaceAlias>namespace1</namespaceAlias>
      	<namespaceAlias>namespace2</namespaceAlias>
      </namespaces>
    </repository>
    <repository>
      <description>iRING Sandbox</description>
      <isReadOnly>true</isReadOnly>
      <name>iRING Sandbox</name>
