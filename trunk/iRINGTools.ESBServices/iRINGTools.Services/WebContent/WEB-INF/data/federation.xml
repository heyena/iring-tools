<?xml version="1.0" encoding="UTF-8"?>
<federation xmlns="http://www.iringtools.org/refdata/federation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://iringtools.org/federation ../schemas/refdata/federation.xsd ">
  <idGenerators>
