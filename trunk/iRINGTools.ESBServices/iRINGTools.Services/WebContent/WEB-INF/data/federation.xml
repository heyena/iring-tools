	  <updateUri>updateUri</updateUri>
      <namespaces>
      	<namespaceAlias>namespace2</namespaceAlias>      	
      </namespaces>
    </repository>
	<repository>
	  <description>RDS/WIP Part 7 Template Library</description>
	  <isReadOnly>true</isReadOnly>
	  <name>Templates</name>
	  <repositoryType>Camelot</repositoryType>
	  <uri>http://tpl.rdlfacade.org/data</uri>
	  <updateUri>updateUri</updateUri>
      <namespaces>
      	<namespaceAlias>namespace1</namespaceAlias>
      	<namespaceAlias>namespace2</namespaceAlias>
      </namespaces>
	</repository>
