@prefix dnr: <http://www.dotnetrdf.org/configuration#> .
<dotnetrdf:/facades/bentley/*> a dnr:HttpHandler ;
        dnr:cacheDuration "0";
        dnr:queryProcessor _:qProcB ;
        dnr:updateProcessor _:uProcB ;
        dnr:protocolProcessor _:pProcB ;
		dnr:stylesheet "../css/interface.css" ;		
        dnr:introText "~/config/BentleyFiatech2011Intro.txt" ;
        dnr:type "VDS.RDF.Web.SparqlServer" ;
        dnr:defaultQueryFile "~/config/AllQuery.txt" .
_:qProcB a dnr:SparqlQueryProcessor ;
        dnr:type "VDS.RDF.Query.LeviathanQueryProcessor" ;
        dnr:usingStore _:sqlStoreB .
_:uProcB a dnr:SparqlUpdateProcessor ;
        dnr:type "VDS.RDF.Update.LeviathanUpdateProcessor" ;
        dnr:usingStore _:sqlStoreB .
_:pProcB a dnr:SparqlHttpProtocolProcessor ;
        dnr:type "VDS.RDF.Update.Protocol.LeviathanProtocolProcessor" ;
        dnr:usingStore _:sqlStoreB .
_:sqlStoreB a dnr:TripleStore ;
        dnr:type "VDS.RDF.SqlTripleStore" ;
        dnr:sqlManager _:sqlServerB .		
_:sqlServerB a dnr:SqlIOManager ;
        dnr:type "VDS.RDF.Storage.MicrosoftSqlStoreManager" ;
        dnr:server ".\SQLEXPRESS" ;
        dnr:database "BentleyFiatech2011Db" ;
        dnr:user "dotNetRDF" ;
        dnr:password "dotNetRDF" .