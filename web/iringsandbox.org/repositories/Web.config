<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="dotNetRDFConfig" value="~/config/config.ttl" />
  </appSettings>

  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <httpHandlers>
      <add path="/repositories/sandbox/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
      <add path="/repositories/geometry/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
      <add path="/repositories/tempInitSet/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
      <add path="/repositories/noumenon/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
	  <add path="/repositories/bechtel/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
	  <add path="/repositories/bentley/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
	  <add path="/repositories/emerson/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
	  <add path="/repositories/dupont/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
	  <add path="/repositories/hatch/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
    </httpHandlers>
    <httpRuntime maxQueryStringLength="8192" requestValidationMode="2.0" />
  </system.web>

  <system.webServer>
    <handlers>
      <add name="/repositories/sandbox/*" path="sandbox/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
      <add name="/repositories/geometry/*" path="geometry/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
      <add name="/repositories/tempInitSet/*" path="tempInitSet/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
      <add name="/repositories/noumenon/*" path="noumenon/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
	  <add name="/repositories/bechtel/*" path="bechtel/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
	  <add name="/repositories/bentley/*" path="bentley/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
	  <add name="/repositories/emerson/*" path="emerson/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
	  <add name="/repositories/dupont/*" path="dupont/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
	  <add name="/repositories/hatch/*" path="hatch/*" verb="*" type="VDS.RDF.Web.SparqlServer" />
    </handlers>
    <modules runAllManagedModulesForAllRequests="true">
      <add name="UrlRoutingModule" type="System.Web.Routing.UrlRoutingModule, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
    </modules>
    <validation validateIntegratedModeConfiguration="false" />
  </system.webServer>
  
</configuration>
